{
  "name": "copy-file-name",
  "displayName": "copy file name",
  "description": "Copy File Names Through  Different Ways",
  "version": "0.0.1",
  "publisher": "tdkirandev",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "repository": "https://github.com/tdkiran/copy-file-name",
  "activationEvents": [
    "onCommand:cfn",
    "onCommand:cffn"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cfn",
        "title": "Copy File Name"
      },
      {
        "command": "cffn",
        "title": "Copy Full File Name (With file Extension)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "true",
          "command": "cfn",
          "group": "cfn@1"
        },
        {
          "when": "true",
          "command": "cffn",
          "group": "cfn@2"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.27",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  }
}